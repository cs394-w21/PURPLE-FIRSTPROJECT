{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "start:pdf": "SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "build:pdf": "SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "test": "react-scripts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "airbnb",
      "airbnb/hooks",
      "prettier"
    ],
    "env": {
      "browser": true,
      "jest": true
    },
    "rules": {
      "react/jsx-one-expression-per-line": 0,
      "react/jsx-curly-newline": 0
    }
  },
  "dependencies": {
    "@react-pdf/renderer": "^1.6.12",
    "babel-eslint": "^10.1.0",
    "eslint-config-prettier": "^7.1.0",
    "expo": "~40.0.0",
    "expo-status-bar": "~1.0.3",
    "firebase": "7.9.0",
    "formik": "^2.2.6",
    "husky": "^4.3.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-web": "~0.13.12",
    "react-scripts": "4.0.1",
    "web-vitals": "^1.1.0",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.0.0"
  },
  "private": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
